<Window x:Class="LDVELH_WPF.LanguageSetting"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:Core="clr-namespace:LDVELH_WPF;assembly=" 
        Title="LanguageSetting" Height="123" Width="398">
    <Grid>
        <Label x:Name="LabelSelectLanguage"  Content="{Core:Translator Text=SelectLanguage}" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <ComboBox x:Name="ComboSupportedLanguage" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" Margin="125,30,0,0"/>
        <Button x:Name="ButtonOK" Content="OK" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="158,61,0,0" Click="ButtonOK_Click"/>

    </Grid>
</Window>
